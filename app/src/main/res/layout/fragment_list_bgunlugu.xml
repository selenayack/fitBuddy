<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textViewListTarih"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:text="Tarih"
        android:textSize="18dp"
        android:textAppearance="?android:attr/textAppearanceLarge"


        />


    <TextView
        android:id="@+id/textViewListToplamKalori"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textViewListTarih"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="5dp"
        android:text="2000"
        android:textAppearance="?android:attr/textAppearanceLarge"


        android:textSize="18dp" />

    <TextView
        android:id="@+id/textViewListProtein"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewListToplamKalori"
        android:layout_alignStart="@+id/textViewListTarih"
        android:layout_marginStart="0dp"
        android:layout_marginTop="4dp"
        android:text="text"
        android:textSize="15dp"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/textViewListYag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewListProtein"
        android:layout_alignStart="@+id/textViewListTarih"
        android:layout_marginStart="0dp"
        android:layout_marginTop="4dp"
        android:text="text"
        android:textSize="15dp"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/textViewListKarb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewListYag"
        android:layout_alignStart="@+id/textViewListTarih"
        android:layout_marginStart="0dp"
        android:layout_marginTop="4dp"
        android:text="text"
        android:textSize="15dp"
        tools:ignore="TextSizeCheck" />



</RelativeLayout>